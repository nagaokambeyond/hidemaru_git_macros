$$cmd[0]  = "";                  $$menu[0]  = "ファイル用：" + leftstr(basename2,70); $$args[0]  = "";
$$cmd[1]  = "";                  $$menu[1]  = "\x01";                      $$args[1]  = "";
$$cmd[2]  = "gitshowdiff.mac";   $$menu[2]  = "(&S)\t git show+diff ";     $$args[2]  = basename2;
