menuarray $$menu,17;
##idx = result;
if (##idx > 0) {
  ##exec_idx = ##idx - 1;
  if ($$cmd[##exec_idx] != "") {
    execmacro currentmacrodirectory + "\\" + $$cmd[##exec_idx], $$args[##exec_idx];
  }
}
endmacro;
