$$cmd[6]  = "gitlog_format.mac"; $$menu[6]  = "(&F)\t git log format";     $$args[6]  = basename2;
$$cmd[7]  = "gitdiff.mac";       $$menu[7]  = "(&D)\t git diff";           $$args[7]  = basename2;
$$cmd[8]  = "gitlog.mac";        $$menu[8]  = "(&L)\t git log";            $$args[8]  = basename2;
